                                        <Border Style="{StaticResource CalculatorTopBorderStyle}">
                                            <DockPanel LastChildFill="False">
                                                <TextBlock Text="综合HP："
                                                           FontSize="25"
                                                           Width="110"
                                                           VerticalAlignment="Center"
                                                           Foreground="#2a73ff" />
                                                <TextBlock Text="1000"
                                                           TextAlignment="Right"
                                                           DockPanel.Dock="Right"
                                                           Width="100"
                                                           Foreground="#2a73ff"
                                                           VerticalAlignment="Center"
                                                           FontSize="25" />
                                            </DockPanel>
                                        </Border>
                                        <Border Style="{StaticResource CalculatorTopBorderStyle}">
                                            <DockPanel LastChildFill="False">
                                                <TextBlock Text="综合ATK:"
                                                           FontSize="25"
                                                           Width="110"
                                                           VerticalAlignment="Center"
                                                           Foreground="#2a73ff" />
                                                <TextBlock Text="1000"
                                                           TextAlignment="Right"
                                                           DockPanel.Dock="Right"
                                                           Width="100"
                                                           Foreground="#2a73ff"
                                                           VerticalAlignment="Center"
                                                           FontSize="25" />
                                            </DockPanel>
                                        </Border>
                                        <CheckBox  Content="拖拽移动"
                                                   IsChecked="{Binding Path=CopyOrMove}"
                                                   Margin="0,0,10,0"
                                                   VerticalAlignment="Center"
                                                   Foreground="#2a73ff"
                                                   DockPanel.Dock="Right"
                                                   FontSize="20"
                                                   pu:CheckBoxHelper.CheckedBorderBrush="#82ADFF"
                                                   pu:CheckBoxHelper.CheckBoxStyle="Switch2"
                                                   pu:CheckBoxHelper.CheckedBackground="#2a73ff"
                                                   pu:CheckBoxHelper.CheckedContent="拖拽复制" />
                                    </DockPanel>
                                </Border>
                            </Grid>

