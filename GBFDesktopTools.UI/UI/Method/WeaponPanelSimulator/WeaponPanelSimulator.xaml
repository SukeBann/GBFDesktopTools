                                                                  pu:ComboBoxHelper.DropDownCornerRadius="5"
                                                                  pu:ComboBoxHelper.CornerRadius="5" />
                                                    </DockPanel>
                                                </Border>
                                            </StackPanel>
                                        </Border>
                                        <Border DockPanel.Dock="Top"
