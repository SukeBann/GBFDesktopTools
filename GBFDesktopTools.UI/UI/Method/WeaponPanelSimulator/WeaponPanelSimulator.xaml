                                              pu:TextBoxHelper.Icon="&#xe9c5;"
                                              pu:IconHelper.FontFamily="{StaticResource PanuonIconFont}"
                                              pu:TextBoxHelper.FocusedBorderBrush="#B5B5B5"
                                              pu:TextBoxHelper.FocusedShadowColor="#B5B5B5"
                                              pu:TextBoxHelper.CanClear="True" />
                                    <Popup x:Name="puInputTip"
                                           StaysOpen="False"
                                           Placement="Bottom"
                                           PopupAnimation="Slide"
