<Window x:Class="Panuon.UI.Silver.Internal.Controls.NoticeWindow"
        x:ClassModifier="internal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:pu="clr-namespace:Panuon.UI.Silver"
        xmlns:local="clr-namespace:Panuon.UI.Silver.Internal.Controls"
        mc:Ignorable="d"
        ShowInTaskbar="False"
        Topmost="True"
        SizeToContent="Width"
        WindowStyle="None"
        Background="Transparent"
        AllowsTransparency="True"
        Initialized="NoticeWindow_Initialized">
    <pu:AnimationStackPanel x:Name="AstpCard"
                            HorizontalAlignment="Right"
                            Direction="Reverse" />
</Window>
