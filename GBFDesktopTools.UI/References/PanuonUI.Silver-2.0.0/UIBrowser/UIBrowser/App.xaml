<Application x:Class="UIBrowser.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UIBrowser"
             xmlns:com="clr-namespace:UIBrowser.Components">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <com:AppBootstrapper x:Key="appBootstrapper" />
                </ResourceDictionary>
                <ResourceDictionary Source="pack://application:,,,/UIBrowser;component/Resources/Control.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
