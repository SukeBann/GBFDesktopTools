    <!--Wave-->
    <Style x:Key="{x:Static irs:ResourceKeys.LoadingWaveRectangleStyle}"
           TargetType="{x:Type Rectangle}">
        <Setter Property="Width"
                Value="{Binding Thickness, RelativeSource={RelativeSource AncestorType=local:Loading},Mode=OneWay}" />
        <Setter Property="RadiusX"
                Value="{Binding CornerRadius,RelativeSource={RelativeSource AncestorType=local:Loading},Mode=OneWay}" />
        <Setter Property="RadiusY"
                Value="{Binding CornerRadius,RelativeSource={RelativeSource AncestorType=local:Loading},Mode=OneWay}" />
        <Setter Property="Fill"
                Value="{Binding Foreground,RelativeSource={RelativeSource AncestorType=local:Loading},Mode=OneWay}" />
        <Setter Property="HorizontalAlignment"
                Value="Center" />
        <Setter Property="RenderTransformOrigin"
                Value="0.5,0.5" />
    </Style>
    <ControlTemplate x:Key="{x:Static irs:ResourceKeys.LoadingWaveTemplate}"
                     TargetType="{x:Type local:Loading}">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition />
                <ColumnDefinition />
                <ColumnDefinition />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Rectangle Style="{StaticResource {x:Static irs:ResourceKeys.LoadingWaveRectangleStyle}}">
                <Rectangle.RenderTransform>
                    <ScaleTransform x:Name="Scale1"
                                    ScaleY="1" />
                </Rectangle.RenderTransform>
            </Rectangle>
            <Rectangle Grid.Column="1"
                       Style="{StaticResource {x:Static irs:ResourceKeys.LoadingWaveRectangleStyle}}">
                <Rectangle.RenderTransform>
                    <ScaleTransform x:Name="Scale2"
                                    ScaleY="1" />
                </Rectangle.RenderTransform>
            </Rectangle>
            <Rectangle  Grid.Column="2"
                        Style="{StaticResource {x:Static irs:ResourceKeys.LoadingWaveRectangleStyle}}">
                <Rectangle.RenderTransform>
                    <ScaleTransform x:Name="Scale3"
                                    ScaleY="1" />
                </Rectangle.RenderTransform>
            </Rectangle>
            <Rectangle Grid.Column="3"
                       Style="{StaticResource {x:Static irs:ResourceKeys.LoadingWaveRectangleStyle}}">
                <Rectangle.RenderTransform>
                    <ScaleTransform x:Name="Scale4"
                                    ScaleY="1" />
                </Rectangle.RenderTransform>
            </Rectangle>
            <Rectangle Grid.Column="4"
                       Style="{StaticResource {x:Static irs:ResourceKeys.LoadingWaveRectangleStyle}}">
                <Rectangle.RenderTransform>
                    <ScaleTransform x:Name="Scale5"
                                    ScaleY="1" />
                </Rectangle.RenderTransform>
            </Rectangle>
        </Grid>
        <ControlTemplate.Triggers>
            <!--Normal Speed-->
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsLoading"
                               Value="True" />
                    <Condition Property="Speed"
                               Value="Normal" />
                </MultiTrigger.Conditions>
                <MultiTrigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard RepeatBehavior="Forever">
                            <DoubleAnimation Storyboard.TargetName="Scale1"
                                             Storyboard.TargetProperty="ScaleY"
                                             To="0.3"
                                             Duration="0:0:0.7"
                                             AutoReverse="True"
                                             RepeatBehavior="Forever" />
                            <DoubleAnimation Storyboard.TargetName="Scale2"
                                             Storyboard.TargetProperty="ScaleY"
                                             To="0.3"
                                             Duration="0:0:0.7"
                                             BeginTime="0:0:0.1"
                                             AutoReverse="True"
                                             RepeatBehavior="Forever" />
                            <DoubleAnimation Storyboard.TargetName="Scale3"
                                             Storyboard.TargetProperty="ScaleY"
                                             To="0.2"
                                             Duration="0:0:0.7"
                                             BeginTime="0:0:0.2"
                                             AutoReverse="True"
                                             RepeatBehavior="Forever" />
                            <DoubleAnimation Storyboard.TargetName="Scale4"
                                             Storyboard.TargetProperty="ScaleY"
                                             To="0.3"
                                             Duration="0:0:0.7"
                                             BeginTime="0:0:0.3"
                                             AutoReverse="True"
                                             RepeatBehavior="Forever" />
                            <DoubleAnimation Storyboard.TargetName="Scale5"
                                             Storyboard.TargetProperty="ScaleY"
                                             To="0.3"
                                             Duration="0:0:0.7"
                                             BeginTime="0:0:0.4"
                                             AutoReverse="True"
                                             RepeatBehavior="Forever" />
                        </Storyboard>
                    </BeginStoryboard>
                </MultiTrigger.EnterActions>
                <MultiTrigger.ExitActions>
                    <BeginStoryboard>
                        <Storyboard RepeatBehavior="Forever">
                            <DoubleAnimation Storyboard.TargetName="Scale1"
                                             Storyboard.TargetProperty="ScaleY"
                                             Duration="0:0:0" />
                            <DoubleAnimation Storyboard.TargetName="Scale2"
                                             Storyboard.TargetProperty="ScaleY"
                                             Duration="0:0:0" />
                            <DoubleAnimation Storyboard.TargetName="Scale3"
                                             Storyboard.TargetProperty="ScaleY"
                                             Duration="0:0:0" />
                            <DoubleAnimation Storyboard.TargetName="Scale4"
                                             Storyboard.TargetProperty="ScaleY"
                                             Duration="0:0:0" />
                            <DoubleAnimation Storyboard.TargetName="Scale5"
                                             Storyboard.TargetProperty="ScaleY"
                                             Duration="0:0:0" />
                        </Storyboard>
                    </BeginStoryboard>

                </MultiTrigger.ExitActions>

            </MultiTrigger>
