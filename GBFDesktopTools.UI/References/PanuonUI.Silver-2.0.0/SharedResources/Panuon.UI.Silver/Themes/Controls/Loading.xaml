            </MultiTrigger>
            <!--SlowSpeed-->
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsLoading"
                               Value="True" />
                    <Condition Property="Speed"
                               Value="Slow" />
                </MultiTrigger.Conditions>
                <MultiTrigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Storyboard.TargetName="Rect1"
                                             Storyboard.TargetProperty="Opacity"
