    <Style x:Key="{x:Static irs:StyleKeys.LoadingStyle}"
           TargetType="{x:Type local:Loading}">
        <Setter Property="Thickness"
                Value="{Binding ActualWidth,Converter={StaticResource {x:Static rs:Converters.DoubleDivideByConverter}}, ConverterParameter=15,RelativeSource={RelativeSource Self},Mode=OneWay}" />
        <Setter Property="CornerRadius"
                Value="0" />
        <Setter Property="Template"
                Value="{StaticResource {x:Static irs:ResourceKeys.LoadingStandardTemplate}}" />
        <Setter Property="Foreground"
                Value="#3E3E3E" />
        <Setter Property="Background"
                Value="#DDDDDD" />
        <Style.Triggers>
            <Trigger Property="LoadingStyle"
                     Value="Wave">
                <Setter Property="Thickness"
                        Value="{Binding ActualWidth,RelativeSource={RelativeSource Self},Converter={StaticResource {x:Static rs:Converters.DoubleDivideByConverter}}, ConverterParameter=7,Mode=OneWay}" />
                <Setter Property="Template"
                        Value="{StaticResource {x:Static irs:ResourceKeys.LoadingWaveTemplate}}" />
            </Trigger>
            <Trigger Property="LoadingStyle"
                     Value="Ring">
                <Setter Property="Percent"
                        Value="0.5" />
                <Setter Property="Thickness"
                        Value="{Binding ActualWidth,Converter={StaticResource {x:Static rs:Converters.DoubleDivideByConverter}}, ConverterParameter=12,RelativeSource={RelativeSource Self},Mode=OneWay}" />
                <Setter Property="Template"
                        Value="{StaticResource {x:Static irs:ResourceKeys.LoadingRingTemplate}}" />
            </Trigger>
            <Trigger Property="LoadingStyle"
                     Value="Ring2">
                <Setter Property="Thickness"
                        Value="{Binding ActualWidth,Converter={StaticResource {x:Static rs:Converters.DoubleDivideByConverter}}, ConverterParameter=12,RelativeSource={RelativeSource Self},Mode=OneWay}" />
                <Setter Property="Template"
                        Value="{StaticResource {x:Static irs:ResourceKeys.LoadingRing2Template}}" />
            </Trigger>
            <Trigger Property="LoadingStyle"
                     Value="Classic">
                <Setter Property="Thickness"
                        Value="{Binding ActualWidth,Converter={StaticResource {x:Static rs:Converters.DoubleDivideByConverter}}, ConverterParameter=10, RelativeSource={RelativeSource Self}, Mode=OneWay}" />
                <Setter Property="Template"
                        Value="{StaticResource {x:Static irs:ResourceKeys.LoadingClassicTemplate}}" />
            </Trigger>
            <Trigger Property="IsLoading"
                     Value="False">
                <Setter Property="Visibility"
                        Value="Hidden" />
            </Trigger>
            <Trigger Property="IsEnabled"
                     Value="False">
                <Setter Property="Opacity"
                        Value="0.4" />
            </Trigger>
        </Style.Triggers>
    </Style>

