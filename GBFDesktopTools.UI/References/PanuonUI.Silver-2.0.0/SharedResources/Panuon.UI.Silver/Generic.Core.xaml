    <Style TargetType="local:SwitchRadioButton"
           BasedOn="{StaticResource {x:Static irs:StyleKeys.SwitchRadioButtonStyle}}" />
    
    <Style TargetType="local:Validator"
           BasedOn="{StaticResource {x:Static irs:StyleKeys.ValidatorStyle}}" />

    <Style TargetType="local:Signboard"
           BasedOn="{StaticResource {x:Static irs:StyleKeys.SignboardStyle}}" />

