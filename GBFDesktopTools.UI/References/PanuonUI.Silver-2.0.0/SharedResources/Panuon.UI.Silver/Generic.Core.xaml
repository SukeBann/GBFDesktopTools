        <core:SharedResourceDictionary Source="pack://application:,,,/Panuon.UI.Silver;component/Themes/Controls/WindowX.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style TargetType="local:Badge"
           BasedOn="{StaticResource {x:Static irs:StyleKeys.BadgeStyle}}" />

    <Style TargetType="local:Breadcrumb"
           BasedOn="{StaticResource {x:Static irs:StyleKeys.BreadcrumbStyle}}" />
    
    <Style TargetType="local:CalendarX"
           BasedOn="{StaticResource {x:Static irs:StyleKeys.CalendarXStyle}}" />

    <Style TargetType="local:DateTimePicker"
           BasedOn="{StaticResource {x:Static irs:StyleKeys.DateTimePickerStyle}}" />

    <Style TargetType="local:Drawer"
           BasedOn="{StaticResource {x:Static irs:StyleKeys.DrawerStyle}}" />
    
    <Style TargetType="local:DropDown"
           BasedOn="{StaticResource {x:Static irs:StyleKeys.DropDownStyle}}" />
    
    <Style TargetType="local:Form"
           BasedOn="{StaticResource {x:Static irs:StyleKeys.FormStyle}}" />
    
    <Style TargetType="local:HorizontalSeparator"
           BasedOn="{StaticResource {x:Static irs:StyleKeys.HorizontalSeparatorStyle}}" />
    
    <Style TargetType="local:Loading"
           BasedOn="{StaticResource {x:Static irs:StyleKeys.LoadingStyle}}" />
    
